def keyPressed():
    global x,y
    if key ==CODED:
        if keyCode== UP:
            y=y-2
        if keyCode==DOWN:
            y=y+2
        if keyCode==RIGHT:
            x=x+2
        if keyCode==LEFT:
            x=x-2
