x1=0
y1=0
w1=0
d1=0
def setup():
     global x1,y1,w1,d1
     size(700,700)
     frameRate(1000000)
     strokeWeight(5)
def draw():
    global x1,y1,w1,d1
    stroke(random(0,255),random(0,255),random(0,255))
    x1=(350)
    y1=(350)
    w1=random(0,1000)
    d1=random(0,1000)
    line(x1,y1,w1,d1)
    x1=x1
    y1=y1
    w1=w1
    d1=d1
